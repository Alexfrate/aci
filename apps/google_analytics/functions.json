    {
        "name": "GOOGLE_ANALYTICS__CUSTOM_DIMENSIONS_CREATE",
        "description": "Creates a custom dimension for the specified property",
        "tags": ["custom", "dimensions", "create"],
        "visibility": "public",
        "active": true,
        "protocol": "rest",
        "protocol_data": {
            "method": "POST",
            "path": "/properties/{propertyId}/customDimensions",
            "server_url": "https://analyticsadmin.googleapis.com/v1beta"
        },
        "parameters": {
            "type": "object",
            "properties": {
                "path": {
                    "type": "object",
                    "properties": {
                        "propertyId": {
                            "type": "string",
                            "description": "Property ID"
                        }
                    },
                    "required": ["propertyId"],
                    "visible": ["propertyId"],
                    "additionalProperties": false
                },
                "body": {
                    "type": "object",
                    "properties": {
                        "parameterName": {
                            "type": "string",
                            "description": "Name of the custom dimension"
                        },
                        "displayName": {
                            "type": "string",
                            "description": "Display name of the custom dimension"
                        },
                        "description": {
                            "type": "string",
                            "description": "Description of the custom dimension"
                        },
                        "scope": {
                            "type": "string",
                            "enum": ["EVENT", "USER"],
                            "description": "Scope of the custom dimension"
                        }
                    },
                    "required": ["parameterName", "displayName", "scope"],
                    "visible": ["parameterName", "displayName", "description", "scope"],
                    "additionalProperties": false
                }
            },
            "required": ["path", "body"],
            "visible": ["path", "body"],
            "additionalProperties": false
        }
