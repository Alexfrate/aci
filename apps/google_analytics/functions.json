        "visibility": "public",
        "active": true,
        "protocol": "rest",
        "protocol_data": {
            "method": "GET",
            "path": "/accounts",
            "server_url": "https://analyticsadmin.googleapis.com/v1beta"
        },
        "parameters": {
            "type": "object",
            "properties": {
                "query": {
                    "type": "object",
                    "properties": {
                        "pageSize": {
                            "type": "integer",
                            "description": "Maximum number of accounts to return",
                            "default": 50
                        },
                        "pageToken": {
                            "type": "string",
                            "description": "Token for next page of results"
                        },
                        "showDeleted": {
                            "type": "boolean",
                            "description": "Whether to include deleted accounts",
                            "default": false
                        }
                    },
                    "required": ["pageSize", "pageToken", "showDeleted"],
                    "visible": ["pageSize", "pageToken", "showDeleted"],
                    "additionalProperties": false
                }
            },
            "required": [],
            "visible": ["query"],
            "additionalProperties": false
        }
    },
