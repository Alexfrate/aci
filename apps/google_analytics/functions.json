    {
        "name": "GOOGLE_ANALYTICS__RUN_REPORT",
        "description": "Generates a customized report of Google Analytics event data",
        "tags": ["reporting", "analytics", "data"],
        "visibility": "public",
        "active": true,
        "protocol": "rest",
        "protocol_data": {
          "method": "POST",
          "path": "/properties/{propertyId}:runReport",
          "server_url": "https://analyticsdata.googleapis.com/v1beta"
        },
        "parameters": {
          "type": "object",
          "properties": {
            "path": {
              "type": "object",
              "properties": {
                "propertyId": {
                  "type": "string",
                  "description": "GA4 property ID (format: 123456789)",
                  "example": "485415204"
                }
              },
              "required": ["propertyId"],
              "visible": ["propertyId"],
              "additionalProperties": false
            },
            "body": {
              "type": "object",
              "properties": {
                "dateRanges": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "startDate": {"type": "string"},
                      "endDate": {"type": "string"}
                    },
                    "required": ["startDate", "endDate"],
                    "visible": ["startDate", "endDate"],
                    "additionalProperties": false
                  },
                  "minItems": 1,
                  "description": "Date range for the report"
                },
                "dimensions": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "name": {"type": "string"}
                    },
                    "required": ["name"],
                    "visible": ["name"],
                    "additionalProperties": false
                  },
                  "description": "Dimensions to group the data by"
                },
                "metrics": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "name": {"type": "string"}
                    },
                    "required": ["name"],
                    "visible": ["name"],
                    "additionalProperties": false
                  },
                  "description": "Metrics to calculate in the report",
                  "minItems": 1
                },
                "limit": {
                  "type": "integer",
                  "description": "Maximum number of rows to return",
                  "default": 1000
                },
                "offset": {
                  "type": "integer",
                  "description": "Row offset for pagination",
                  "default": 0
                }
              },
              "required": ["dateRanges", "metrics"],
              "visible": ["dateRanges", "dimensions", "metrics", "limit", "offset"],
              "additionalProperties": false
            }
          },
          "required": ["path", "body"],
          "visible": ["path", "body"],
          "additionalProperties": false
        }
      }
]