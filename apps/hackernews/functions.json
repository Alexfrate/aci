{
    "functions": [
        {
            "name": "HACKERNEWS__GET_TOP_STORIES",
            "description": "Retrieve the top stories from Hacker News",
            "tags": ["hackernews", "stories", "top"],
            "visibility": ["public"],
            "active": true,
            "protocol": "rest",
            "protocol_data": {
                "method": "GET",
                "path": "/topstories.json",
                "server_url": "https://hacker-news.firebaseio.com/v0"
            },
            "parameters": {
                "type": "object",
                "properties": {
                    "query": {
                        "type": "object",
                        "properties": {
                            "limit": {
                                "type": "integer",
                                "description": "Number of top stories to retrieve",
                                "default": 10
                            }
                        }
                    }
                },
                "additionalProperties": false
            }
        },
